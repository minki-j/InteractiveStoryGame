# LangChain configuration
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT="your_project_name"
LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"
LANGCHAIN_API_KEY="your_langchain_api_key"

# API keys
OPENAI_API_KEY="your_openai_api_key"
ANTHROPIC_API_KEY="your_anthropic_api_key"

# LLM settings
LLM_TEMPERATURE=0.7

# Default and fallback models
DEFAULT_MODEL="claude-3-5-sonnet-20240620"
FALLBACK_MODEL="gpt-4o-2024-08-06"

# Uncomment and modify to use alternative models
# DEFAULT_MODEL="claude-3-haiku-20240307"
# DEFAULT_MODEL="claude-3-opus-20240229"
# DEFAULT_MODEL="gpt-4o-mini"
# DEFAULT_MODEL="gpt-4o-2024-08-06"

# Uncomment to change fallback model
# FALLBACK_MODEL="claude-3-5-sonnet-20240620"

# Development settings
SKIP_LLM_CALLINGS=false

AUTH_CLIENT_ID=
AUTH_CLIENT_SECRET=